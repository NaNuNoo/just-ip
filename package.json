{
  "name": "just-ip",
  "version": "0.1.0",
  "description": "An IP library for both browser and nodejs.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NaNuNoo/just-ip.git"
  },
  "keywords": [
    "IP",
    "IP address",
    "IP browser",
    "network"
  ],
  "author": "NaNuNoo",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/NaNuNoo/just-ip/issues"
  },
  "homepage": "https://github.com/NaNuNoo/just-ip#readme",
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "@types/node": "^9.3.0",
    "jest": "^22.0.6",
    "ts-jest": "^22.0.1",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "mapCoverage": true
  },
  "scripts": {
    "build": "./node_modules/.bin/tsc && echo \"Build Just-IP success !\"",
    "test": "./node_modules/.bin/jest --coverage"
  }
}
